{% extends "layout.twig" %}

{% block content %}
	<h2>Users</h2>
	<a class="btn btn-outline-primary" href="{{ 'admin/users/create' | url }}">New User</a>
	<table class="table">
		<caption>Table content blogs</caption>
		<thead>
			<tr>
				<th>Name</th>
				<th>Email</th>
				<th>Edit</th>
				<th>Delete</th>
			</tr>
		</thead>
		<tbody>
			{% for user in users %}
				<tr>
					<td> {{ user.name }} </td>
					<td> {{ user.email }} </td>
					<td>Edit</td>
					<td>Delete</td>
				</tr>

			{% endfor %}
		</tbody>
	</table>

{% endblock %}